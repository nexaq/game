{
  "name": "game",
  "version": "1.0.0",
  "scripts": {
    "build:dev:vendors": "NODE_ENV=development webpack --mode=development --config webpack/config/vendors.config.ts",
    "build:prod:vendors": "NODE_ENV=production webpack --mode=production --config webpack/config/vendors.config.ts",
    "build:configs": "tsc --project configs",
    "build:ssr:server": "tsc --project ssr",
    "build:extras": "webpack --config webpack/config/extras.config.ts",
    "ssr:dev": "better-npm-run ssr:dev",
    "watch:ssr:dev": "better-npm-run watch:ssr:dev",
    "start:ssr:dev": "NODE_PATH=./dist NODE_OPTIONS=--enable-source-maps CFG_DIR=$(pwd)/dist/configs node dist/ssr/index.js",
    "ssr:prod": "better-npm-run ssr:prod",
    "build:ssr:prod": "better-npm-run build:ssr:prod",
    "start:ssr:prod": "NODE_ENV=production NODE_PATH=./dist NODE_OPTIONS=--enable-source-maps CFG_DIR=$(pwd)/dist/configs node dist/ssr/index.js",
    "build:client:prod": "NODE_ENV=production webpack --mode=production --config webpack/config/client.config.ts",
    "backend:dev": "better-npm-run backend:dev",
    "start:backend:dev": "NODE_PATH=./dist NODE_OPTIONS=--enable-source-maps CFG_DIR=$(pwd)/dist/configs nodemon dist/backend/index.js",
    "backend:prod": "better-npm-run backend:prod",
    "start:backend:prod": "NODE_ENV=production NODE_PATH=./dist NODE_OPTIONS=--enable-source-maps CFG_DIR=$(pwd)/dist/configs nodemon dist/backend/index.js",
    "clean": "rm -rf dist && mkdir dist",
    "precommit": "npm-run-all lint tsc",
    "tsc": "node ./node_modules/.bin/tsc --noEmit",
    "lint": "npm-run-all lint:*",
    "lint:style": "npx stylelint './**/*.{css,scss}'",
    "lint:src": "npx eslint --ext=.ts,.tsx ./",
    "fix": "npm-run-all fix:lint:*",
    "fix:lint:src": "npx eslint --fix --ext=.ts,.tsx ./",
    "fix:lint:style": "npx stylelint './**/*.{css,scss}' --fix",
    "test": "npm-run-all lint:* test:*",
    "test:mocha": "echo \"Error: no test specified\""
  },
  "engines": {
    "node": ">= 14.18.0"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/plugin-transform-runtime": "^7.16.10",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-react": "^7.16.0",
    "@babel/preset-typescript": "^7.16.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.2",
    "@types/compression-webpack-plugin": "^6.0.6",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.12",
    "@types/duplicate-package-checker-webpack-plugin": "^2.1.2",
    "@types/express-fileupload": "^1.2.2",
    "@types/express-rate-limit": "^5.1.3",
    "@types/htmlescape": "^1.1.1",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/lodash.flow": "^3.5.6",
    "@types/lodash.merge": "^4.6.6",
    "@types/mini-css-extract-plugin": "^2.2.0",
    "@types/node": "^15.14.9",
    "@types/react": "^17.0.19",
    "@types/react-dom": "^17.0.9",
    "@types/react-helmet": "^6.1.2",
    "@types/react-hot-loader": "^4.1.1",
    "@types/react-loadable": "^5.5.6",
    "@types/react-redux": "^7.1.22",
    "@types/react-sticky": "^6.0.4",
    "@types/redux-logger": "^3.0.9",
    "@types/redux-thunk": "^2.1.0",
    "@types/serialize-javascript": "^5.0.2",
    "@types/uuid": "^8.3.3",
    "@types/webpack": "5.28.0",
    "@types/webpack-dev-middleware": "^5.0.2",
    "@types/webpack-dev-server": "^3.11.5",
    "@types/webpack-hot-middleware": "^2.25.5",
    "@types/webpack-node-externals": "^2.5.2",
    "autoprefixer": "^10.4.2",
    "babel-loader": "^8.2.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "better-npm-run": "^0.1.1",
    "compression-webpack-plugin": "^8.0.1",
    "concurrently": "^7.0.0",
    "css-hot-loader": "^1.4.4",
    "css-loader": "^6.3.0",
    "duplicate-package-checker-webpack-plugin": "^3.0.0",
    "file-loader": "^6.2.0",
    "fork-ts-checker-webpack-plugin": "^6.3.2",
    "merge-options": "^3.0.4",
    "mini-css-extract-plugin": "^2.2.0",
    "nodemon": "^2.0.13",
    "npm-run-all": "^4.1.5",
    "null-loader": "^4.0.1",
    "postcss": "^8.4.0",
    "postcss-functions": "^4.0.2",
    "postcss-import": "^14.0.2",
    "postcss-loader": "^6.1.1",
    "postcss-nested": "^5.0.6",
    "postcss-simple-vars": "^6.0.3",
    "react-loadable": "^5.5.0",
    "react-refresh": "^0.11.0",
    "remove-files-webpack-plugin": "^1.5.0",
    "ts-loader": "^9.2.5",
    "ts-node": "^10.2.1",
    "tsconfig-paths-webpack-plugin": "^3.5.1",
    "type-fest": "^2.5.4",
    "typescript": "^4.3.4",
    "typings-for-css-modules-loader": "^1.7.0",
    "webpack": "^5.64.2",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.7.3",
    "webpack-hot-middleware": "^2.25.1",
    "webpack-node-externals": "^3.0.0"
  },
  "dependencies": {
    "array-to-tree": "^3.3.2",
    "body-parser": "^1.19.1",
    "connected-react-router": "^6.9.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-fileupload": "^1.3.1",
    "express-rate-limit": "^5.3.0",
    "helmet": "^4.6.0",
    "htmlescape": "^1.1.1",
    "js-sha256": "^0.9.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "lodash.flow": "^3.5.0",
    "lodash.merge": "^4.6.2",
    "mongoose": "^6.1.8",
    "pg": "^8.7.1",
    "postcss-for": "^2.1.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-hot-loader": "^4.13.0",
    "react-redux": "^7.2.6",
    "react-router": "^6.0.2",
    "react-router-dom": "^6.0.2",
    "react-sticky": "^6.0.3",
    "redux": "^4.1.2",
    "redux-logger": "^3.0.6",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.14.0",
    "sequelize-cli": "^6.4.1",
    "sequelize-typescript": "^2.1.2",
    "serialize-javascript": "^6.0.0",
    "uuid": "^8.3.2"
  },
  "betterScripts": {
    "ssr:dev": {
      "command": "npm-run-all build:configs build:ssr:server build:extras && npm-run-all --parallel --print-name watch:ssr:dev start:ssr:dev",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "watch:ssr:dev": {
      "command": "webpack --watch --config webpack/config/ssr.config.ts",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "ssr:prod": {
      "command": "npm-run-all build:configs build:ssr:server build:extras && npm-run-all --parallel --print-name build:client:prod build:ssr:prod start:ssr:prod",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "build:ssr:prod": {
      "command": "webpack --mode=production --config webpack/config/ssr.config.ts",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "backend:dev": {
      "command": "concurrently \"tsc --project backend --watch\" \"npm run start:backend:dev\"",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "backend:prod": {
      "command": "tsc --project backend && npm run start:backend:prod",
      "env": {
        "NODE_ENV": "production"
      }
    }
  }
}
